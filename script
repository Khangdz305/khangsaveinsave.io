-- ===== Unlock Logic =====
button.MouseButton1Click:Connect(function()
	if input.Text == password then
		title.Text = "âœ… Access Granted"
		status.Text = "ğŸ”“ Unlocked. Click 'Copy with Scripts!'"
		copyButton.Visible = true
		button.Visible = false
		input.Visible = false
		log("Access Granted!")
	else
		attempts += 1
		status.Text = "âŒ Wrong Password ("..attempts.."/3)"
		log("Wrong Password Attempt "..attempts)
		if attempts >= 3 then
			title.Text = "â›” Too Many Failed Attempts"
			log("GUI Locked due to too many attempts")
			wait(1)
			gui:Destroy()
		end
	end
end)

-- ===== Copy Button Logic =====
